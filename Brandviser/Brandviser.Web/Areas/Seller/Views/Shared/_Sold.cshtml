@model IEnumerable<Brandviser.Web.Areas.Seller.Models.PartialPricedDatedDomainViewModel>

<div class="table-responsive">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Domain Name</th>
                <th>Status</th>
                <th>Sold On</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var domain in Model)
            {
                <tr>
                    <td>@domain.Name</td>
                    <td>@domain.Status</td>
                    <td>@domain.SoldOn.ToString("dd/MMM/yyyy")</td>
                    <td>$@domain.Price</td>
                </tr>
            }
        </tbody>
    </table>
</div>
