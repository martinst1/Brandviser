
<h3>My domains</h3>
<hr />
<div class="btn-group">
    @Ajax.ActionLink("Pending Approval", "PendingApproval", "Admin", null, new AjaxOptions()
{
    HttpMethod = "Get",
    AllowCache = false,
    InsertionMode = InsertionMode.Replace,
    UpdateTargetId = "domain-partial",
    LoadingElementId = "spinner"
}, new { @class = "btn btn-sm btn-primary" })

    @Ajax.ActionLink("Pending Logo Approval", "PendingLogoApproval", "Admin", null, new AjaxOptions()
{
    HttpMethod = "Get",
    AllowCache = false,
    InsertionMode = InsertionMode.Replace,
    UpdateTargetId = "domain-partial",
    LoadingElementId = "spinner"
}, new { @class = "btn btn-sm btn-primary" })
</div>
<div id="domain-partial"></div>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/unobtrusive")